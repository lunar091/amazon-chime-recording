{
  "name": "amazon-chime-sdk-js-demos",
  "version": "0.1.0",
  "description": "Amazon Chime SDK JavaScript Demos",
  "scripts": {
    "deps": "node script/barrelize.js && npm run tsc:src",
    "copy-assets": "node script/copy-assets.js",
    "tsc:src": "tsc --build tsconfig.json",
    "build:fast": "npm run copy-assets && node script/webpack-wrapper.js",
    "build": "npm run deps && npm install && tsc && npm run build:fast",
    "start:fast": "npm run copy-assets && node script/webpack-wrapper.js --serve",
    "start:hot": "npm run copy-assets && webpack serve --config ./webpack.config.hot.js",
    "start": "npm run deps && npm install && npm run start:fast"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "cross-spawn": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "markdown-it": "^11.0.1",
    "open-iconic": "^1.1.1",
    "postcss-loader": "^6.1.1",
    "react-dev-utils": "^11.0.4",
    "sass": "^1.26.11",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "@types/offscreencanvas": "^2019.6.4",
    "amazon-chime-sdk-js": "^2.23.0",
    "aws-sdk": "^2.1006.0",
    "bootstrap": "^4.5.2",
    "compression": "^1.7.4",
    "fs-extra": "^10.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "offscreen-canvas": "^0.1.1",
    "popper.js": "^1.15.0",
    "react-avatar": "^4.0.0",
    "sha1": "^1.1.1",
    "ui-avatar-svg": "^1.0.0",
    "ui-avatars": "^1.0.0",
    "util": "^0.12.4"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/aws/amazon-chime-sdk-js"
  }
}
